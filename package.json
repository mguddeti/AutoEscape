	],
	"activationEvents": [
		"onCommand:escapesquared.escapesquared",
		"onCommand:escapesquared.copiedtext",
		"onCommand:escapesquared.highlightedtext"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "escapesquared.escapesquared",
				"title": "EscapeSquared"
